<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyWidth="10%"
    android:keyHeight="10%"
    android:verticalGap="0px">

    <Row>
        <Key
            android:codes="-112"
            android:keyWidth="8.3%p"
            android:keyEdgeFlags="left"
            android:keyLabel="F1" />
        <Key
            android:codes="-113"
            android:keyWidth="8.3%p"
            android:keyLabel="F2" />
        <Key
            android:codes="-114"
            android:keyWidth="8.3%p"
            android:keyLabel="F3" />
        <Key
            android:codes="-115"
            android:keyWidth="8.3%p"
            android:keyLabel="F4" />
        <Key
            android:codes="-116"
            android:keyWidth="8.3%p"
            android:keyLabel="F5" />
        <Key
            android:codes="-117"
            android:keyWidth="8.3%p"
            android:keyLabel="F6" />
        <Key
            android:codes="-118"
            android:keyWidth="8.3%p"
            android:keyLabel="F7" />
        <Key
            android:codes="-119"
            android:keyWidth="8.3%p"
            android:keyLabel="F8" />
        <Key
            android:codes="-120"
            android:keyWidth="8.3%p"
            android:keyLabel="F9" />
        <Key
            android:codes="-121"
            android:keyWidth="8.3%p"
            android:keyLabel="F10" />
        <Key
            android:codes="-122"
            android:keyWidth="8.3%p"
            android:keyLabel="F11" />
        <Key
            android:codes="-123"
            android:keyWidth="8.7%p"
            android:keyEdgeFlags="right"
            android:keyLabel="F12" />
    </Row>

    <Row>
        <Key
            android:codes="0x09"
            android:keyWidth="15%p"
            android:keyEdgeFlags="left"
            android:keyLabel="Tab" />
        <Key
            android:codes="-27"
            android:keyWidth="19%p"
            android:keyLabel='Esc' />
        <Key
            android:codes="-36"
            android:keyWidth="17%p"
            android:keyLabel="Home" />
        <Key
            android:codes="-38"
            android:keyWidth="17%p"
            android:keyLabel="Up" />
        <Key
            android:codes="-35"
            android:keyWidth="17%p"
            android:keyLabel="End" />
        <Key
            android:codes="-46"
            android:keyWidth="15%p"
            android:keyEdgeFlags="right"
            android:keyLabel="Del" />
    </Row>

    <Row>
        <Key
            android:codes="-16"
            android:keyWidth="15%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_shift" />
        <Key
            android:codes="-44"
            android:keyWidth="19%p"
            android:keyLabel="PrtSc" />
        <Key
            android:codes="-37"
            android:keyWidth="17%p"
            android:keyLabel="Left" />
        <Key
            android:codes="-40"
            android:keyWidth="17%p"
            android:keyLabel='Down' />
        <Key
            android:codes="-39"
            android:keyWidth="17%p"
            android:keyLabel="Right" />
        <Key
            android:codes="-8"
            android:isRepeatable="true"
            android:keyWidth="15%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="-17"
            android:isModifier="true"
            android:isSticky="true"
            android:keyWidth="13%p"
            android:keyEdgeFlags="left"
            android:keyLabel="Ctrl" />
        <Key
            android:codes="-3"
            android:keyWidth="12%p"
            android:keyLabel="ABC" />
        <Key
            android:codes="-1"
            android:keyWidth="12%p"
            android:keyLabel=".?123" />
        <Key
            android:codes="-32"
            android:isRepeatable="true"
            android:keyWidth="23%p"
            android:keyIcon="@drawable/sym_keyboard_space" />
        <Key
            android:codes="0x2E"
            android:keyWidth="10%p"
            android:keyLabel="." />
        <Key
            android:codes="-18"
            android:isModifier="true"
            android:isSticky="true"
            android:keyWidth="13%p"
            android:keyLabel="Alt" />
        <Key
            android:codes="-13"
            android:keyWidth="17%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_return" />
    </Row>
</Keyboard>  
